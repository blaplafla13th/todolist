<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="702.0" prefWidth="447.0"
            style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/18"
            fx:controller="blaplafla.todolist.views.gui.DoneSub">
    <ListView fx:id="listViewTask" layoutX="9.0" layoutY="132.0" prefHeight="518.0" prefWidth="429.0"
              AnchorPane.leftAnchor="9.0" AnchorPane.rightAnchor="9.0"/>
    <GridPane layoutX="9.0" layoutY="657.0" prefHeight="25.0" prefWidth="429.0" AnchorPane.leftAnchor="9.0"
              AnchorPane.rightAnchor="9.0" AnchorPane.topAnchor="657.0" GridPane.rowIndex="2">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label fx:id="prev" onMouseClicked="#prev" text="Prev">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label fx:id="next" onMouseClicked="#next" text="Next" textAlignment="RIGHT" GridPane.columnIndex="2">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label fx:id="pageLabel" layoutX="10.0" layoutY="10.0" text="Page 1" GridPane.columnIndex="1">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
    </GridPane>
    <GridPane layoutX="9.0" layoutY="43.0" prefHeight="30.0" prefWidth="409.0" AnchorPane.leftAnchor="9.0"
              AnchorPane.rightAnchor="9.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label fx:id="undone" text="Done list">
            <font>
                <Font name="System Bold" size="20.0"/>
            </font>
        </Label>
        <Button fx:id="refresh" mnemonicParsing="false" onAction="#addData" prefHeight="30.0" prefWidth="80.0"
                text="Refresh" GridPane.columnIndex="1"/>
    </GridPane>
    <GridPane layoutX="9.0" layoutY="85.0" AnchorPane.leftAnchor="9.0" AnchorPane.rightAnchor="9.0">
        <columnConstraints>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0"/>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0"/>
            <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Button fx:id="detail" mnemonicParsing="false" onAction="#detail" prefWidth="80.0" text="Detail"
                GridPane.columnIndex="2"/>
        <Button fx:id="toggle" mnemonicParsing="false" onAction="#toggle" prefWidth="80.0" text="Toggle"
                GridPane.columnIndex="1"/>
        <Button fx:id="delete" mnemonicParsing="false" onAction="#delete" prefWidth="80.0" text="Delete"/>
    </GridPane>
    <Label fx:id="mainTaskName" layoutX="9.0" layoutY="14.0" text="Task name: blablabla" AnchorPane.leftAnchor="9.0"
           AnchorPane.topAnchor="14.0">
        <font>
            <Font name="System Bold" size="20.0"/>
        </font>
    </Label>
</AnchorPane>
